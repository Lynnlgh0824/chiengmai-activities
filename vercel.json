{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "devCommand": "echo 'No dev command needed'",
  "outputDirectory": "public",
  "installCommand": "echo 'Skip install'",
  "framework": null,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ],
  "headers": [
    {
      "source": "/data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=86400"
        }
      ]
    }
  ]
}
